<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
{{ range where .Site.RegularPages "Section" "posts" }}
  <url>
    <loc>{{ .Permalink }}</loc>
    <lastmod>{{ .Lastmod.Format "2006-01-02" }}</lastmod>
    <priority>0.6</priority>
  </url>
{{ end }}
</urlset>
